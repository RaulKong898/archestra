{
  "request": {
    "method": "POST",
    "urlPathPattern": "/bedrock/model/.*/converse",
    "headers": {
      "x-amz-access-key-id": {
        "contains": "bedrock-compression"
      }
    },
    "bodyPatterns": [
      {
        "contains": "\"files\":[{\"name\":\"README.md\",\"size\":1024"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "$metadata": {
        "httpStatusCode": 200,
        "requestId": "test-compression-disabled"
      },
      "output": {
        "message": {
          "role": "assistant",
          "content": [
            {
              "text": "I have processed the uncompressed JSON tool result."
            }
          ]
        }
      },
      "stopReason": "end_turn",
      "usage": {
        "inputTokens": 150,
        "outputTokens": 10
      }
    }
  }
}
